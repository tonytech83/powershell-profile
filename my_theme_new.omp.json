{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "closer": "p:gray",
    "gray": "#84868b",
    "green": "#4ce92d",
    "lavender": "#B4BEFE",
    "new_orange": "#FF9248",
    "new_red": "#bd3220",
    "orange": "#d08770",
    "pink": "#F5C2E7",
    "red": "#ff5555",
    "white": "#ffffff"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500",
          "foreground": "p:new_orange",
          "type": "text"
        },
        // venv
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:new_orange",
          "template": "{{ if .Venv }}[ {{ end }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:green",
          "template": "{{ if .Venv }}venv{{ end }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:new_orange",
          "template": "{{ if .Venv }} ]{{ end }}"
        },
        // user and hostname
        {
          "style": "plain",
          "template": "[ ",
          "foreground": "p:new_orange",
          "type": "text"
        },
        {
          "style": "plain",
          "template": "<p:lavender>{{ .UserName }}</>",
          "type": "session"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{.Icon}} ",
          "foreground": "p:new_orange",
          "type": "os",
          "style": "plain"
        },
        {
          "style": "plain",
          "template": "<p:lavender>{{ .HostName }}</>",
          "type": "session"
        },
        {
          "style": "plain",
          "template": " ]",
          "foreground": "p:new_orange",
          "type": "text"
        },
        // path
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": "[ {{ .Path }} ] ",
          "foreground": "p:gray",
          "type": "path",
          "style": "plain"
        },
        // git
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true
          },
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:red",
          "type": "git",
          "style": "plain"
        },
        // node
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": "<#ffffff>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#98C379",
          "type": "node",
          "style": "plain"
        },
        // go
        {
          "foreground": "#8ED1F7",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        // julia
        {
          "foreground": "#4063D8",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "\ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        // python
        {
          "foreground": "p:blue",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        // ruby
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        // azfunc
        {
          "foreground": "#FEAC19",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "plain",
          "template": "\uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        // aws
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500ï„¸ ",
          "foreground": "p:new_orange",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
